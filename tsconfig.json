{
  "compilerOptions": {
    "lib": [
      "dom",
      "es5",
      "es2015",
      "es2015.promise"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es5",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "components/*": ["./src/components/*"],
      "asset/*": ["./src/asset/*"],
      "sass": [ "./src/sass"],
      "router": ["./src/router"],
      "store/*": ["./src/store/*"]
    }
  }
}